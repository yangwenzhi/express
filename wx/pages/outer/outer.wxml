<!--index.wxml-->
<view class="container">
    <view  bindtap="bindViewTap" class="userinfo">
        <image class="userinfo-avatar" src="../../images/3.png" background-size="cover"></image>
    </view>
    <form bindsubmit="formSubmit" wx:if="{{info.productid}}">
        <view class="usermotto2">
            <view><text class="user-motto">扫码结果</text></view>
        </view>
        <view class="usermotto">
            <view><text class="user-motto">快递公司：{{info.name || '-'}}</text></view>
            <view><text class="user-motto">快递单号：{{info.productid}}</text></view>
            <view><text class="user-motto">入库时间：{{info.createtime || '-'}}</text></view>
        </view>
        <input type="text" name="productid" value="{{info.productid}}" class="hidden" />
        <button formType="submit">确认出库</button>
    </form>
</view>
