.container{
    padding: 0;
}

.item {
    width: 100%;
    line-height: 60rpx;
    float: left;
    padding: 30rpx;
    box-sizing: border-box;
    position: relative;
    background: #ffffff;
    font-size: 32rpx;
    margin-bottom: 10rpx;
}

.btn {
    width: 100%;
    height: 128rpx;
    position: fixed;
    bottom: 0;
    left: 0;
}

.btn-save {
    width: 50%;
    height: 128rpx;
    line-height: 128rpx;
    background: #e64340;
    color: white;
    font-size: 32rpx;
    border-radius: 0;
    float: left;
    border: 0;
    text-align: center;
}

.btn-save1{
    background: #000;
    color: #aaa;
}
